<%= form_for photo, html: {multipart: true } do |f| %>

  <p><%= f.file_field :image, accept: "image/*" %></p>

  <%= f.hidden_field :restaurant %>
  <input id="searchTextField" id="pac-input" class="controls" type="text" placeholder="search for restaurant here." >


  <p><%= f.text_field :foodtags, value: nil, :placeholder => "Foodtags" %></p>
  <p>Price Range: <%= f.select :pricetag, [['$0..$10', "$" ], ['$10..$15', "$$" ], ['$15..$20', "$$$" ],['$20+', "$$$$" ]] %></p>
  <p><%= f.text_field :dish_name, value: nil, :placeholder => "Dish Name" %></p>
     <%= f.hidden_field :lat, value: "", id:"lat_field"  %>
     <%= f.hidden_field :lng, value: "", id:"lng_field"  %>
  <p><%= submit_tag 'Add Photo', id:"add_photo_button" %></br>

<% end %>
